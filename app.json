{
  "stack": "heroku-24",
  "name": "circle-songs",
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "heroku/ruby"
    }
  ],
  "scripts": {
    "postdeploy": "bundle exec rake db:schema:load db:seed"
  },
  "env": {
    "RAILS_MASTER_KEY": {
      "required": true
    },
    "FONTAWESOME_NPM_AUTH_TOKEN": {
      "required": true
    },
    "NODE_ENV": "production",
    "RAILS_ENV": "production"
  },
  "addons": [
    "heroku-postgresql"
  ]
}
